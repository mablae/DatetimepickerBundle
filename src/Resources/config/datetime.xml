<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="mablae_datetimepicker.form.options" type="collection" />
    </parameters>

    <services>

        <service id="mablae_datetimepicker.form.type.datetime" class="Mablae\DatetimepickerBundle\Form\Type\DatetimeType">
            <tag name="form.type" alias="mablae_datetime" />
            <argument>%mablae_datetimepicker.form.options%</argument>
        </service>
        
    </services>

</container>
